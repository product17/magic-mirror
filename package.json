{
  "name": "base_node_project",
  "version": "1.0.0",
  "description": "User Server for multi site setup",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "check-npm": "nsp check",
    "build-server": "npm run clean && node_modules/babel-cli/bin/babel.js -d ./build ./server -w",
    "start": "NODE_ENV=production node ./build/index.js",
    "start-debug": "node --debug ./build/index.js",
    "start-dev": "NODE_ENV=development node_modules/babel-cli/bin/babel-node.js ./server/index.js",
    "start-dev-debug": "NODE_ENV=development node_modules/babel-cli/bin/babel-node.js --debug ./server/index.js",
    "run-dev": "npm run build-server && npm run start-dev",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.js --watch"
  },
  "author": "Brent Crocker",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "bunyan": "^1.8.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.13.0",
    "helmet": "^2.1.1",
    "jade": "^1.11.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "redis": "^2.6.2",
    "socket.io": "^1.5.0"
  }
}
